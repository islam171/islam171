define([],function(){"use strict";return function(t,e){Array.prototype.forEach.call(t,function(t){t.getAttribute("data-limitation")&&t.getAttribute("data-limitation")<e?t.textContent=t.getAttribute("data-limitation"):t.textContent=e,t.setAttribute("data-count",e),e<=0?t.classList.add("d-none"):t.classList.remove("d-none")})}});